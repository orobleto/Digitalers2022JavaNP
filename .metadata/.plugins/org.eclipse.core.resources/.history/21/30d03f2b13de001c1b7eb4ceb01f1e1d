
public class Operadores {

	public static void main(String[] args) {
		double numeroA = 5;
		double numeroB = 8;

		double total = numeroA + numeroB;

		System.out.println(total);

		total = total + 5;

		System.out.println(total);

		total = total - 4.3;// total + 5 *36 /(total +8);

		System.out.println(total);

		// operadores de asginacion
		// total = total - 4.3;
		total -= 4.3;
		System.out.println();

	}

}

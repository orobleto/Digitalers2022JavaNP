
public class Asignacion {

	public static void main(String[] args) {
		int numero = 0;
		System.out.println(numero);
		
		numero = numero + 1;
		System.out.println(numero);
		
		numero+=1;
		System.out.println(numero);
		
		numero++;
		System.out.println(numero);
		
		numero--;
		System.out.println();

	}

}

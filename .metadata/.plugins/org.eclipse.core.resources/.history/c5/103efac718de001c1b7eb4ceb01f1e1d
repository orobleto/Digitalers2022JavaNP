
public class OperadoresLogicos {

	public static void main(String[] args) {
		/*
		 * Iphone y Rojo
		 */
		boolean iphone;
		boolean rojo;
		// AND OR
		// &&

		iphone = true;
		rojo = true;
		//
		System.out.println(iphone && rojo);


		iphone = true;
		rojo = true;
		//
		System.out.println(iphone && rojo);
		
	}

}
